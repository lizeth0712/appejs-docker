{
  "dependencies": {
    "axios": "^1.8.4",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.4.7",
    "ejs": "^3.1.10",
    "express": "^4.21.2",
    "express-session": "^1.18.1",
    "mongoose": "^8.13.1",
    "mysql2": "^3.13.0",
    "pdfkit": "^0.16.0"
  },
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "cross-env NODE_ENV=test jest --runInBand --detectOpenHandles"
  },
  "jest": {
    "testEnvironment": "node",
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "routes/**/*.js",
      "config/**/*.js",
      "models/**/*.js",
      "app.js"
    ],
    "moduleNameMapper": {
      "\\.(css|png|jpg|jpeg)$": "<rootDir>/test/__mocks__/fileMock.js"
    }
  },
  "devDependencies": {
    "cross-env": "^10.0.0",
    "jest": "^30.1.3",
    "supertest": "^7.1.4"
  }
}
